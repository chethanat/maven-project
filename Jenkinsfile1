pipeline{
  agent any
  triggers {
      pollSCM(* * * * *)
  }
  stages {
    stage('Build') {
      steps{
        script{
          echo "build"
        }
      }
    }
    stage('archieve'){
      steps{
        script{
          echo "archieve"
      }
    }
    }
    stage('Deploy to dev'){
     when {
       branch 'dev'
     }
     steps {
       echo 'deploy to dev'
    }
}
    stage('Deploy to master'){
     when {
       branch 'master'
     }
     steps {
       echo 'deploy to master'
    }
}
   stage('Deploy to release'){
     when {
       branch 'release'
     }
     steps {
       echo 'deploy to release '
    }
}
  
}
}
